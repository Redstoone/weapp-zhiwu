page {
  background: #fff;
  color: #666;
  font-size: 24rpx;
  line-height: 34rpx;
}
image {
  width: 100%;
}
.clear::after {
  clear: both;
  display: block;
  visibility: hidden;
  overflow: hidden;
  height: 0;
  content: "";
}
.left {
  float: left;
}
.right {
  float: right;
}

.page-bd-spacing {
  margin-left: 20rpx;
  margin-right: 20rpx;
}

/* title */
.box-title {
  margin: 40rpx 20rpx;
  padding: 20rpx 30rpx;
  background-color: #252525;
}
.bt-star {
  line-height: 42rpx;
  font-size: 30rpx;
  color: #fff;
  font-weight: bold;
}
.bt-more {
  height: 40rpx;
  line-height: 40rpx;
  color: #fff;
  width: 100rpx;
  text-align: right;
}

/* index tab */
.index-tab {
  height: 88rpx;
}
.index-tab .weui-navbar__item {
  text-align: center;
  font-size: 15px;
}
.search-bar {
  background-color: #fff;
  border-top: none;
  border-bottom: none;
  padding: 30rpx 20rpx 10rpx;
}

/* goods list */
.goods-list {
  margin: 20rpx;
}
.goods-item {
  width: 345rpx;
  position: relative;
  margin-right: 20rpx;
}
.goods-item-head {
  position: absolute;
  left: 30rpx;
  top: 330rpx;
  line-height: 50rpx;
  color: #fff;
}
.goods-item-img {
  width: 50rpx;
  height: 50rpx;
  overflow: hidden;
  border-radius: 50%;
  margin-right: 20rpx;
}
.goods-item-img-image {
  width: 50rpx;
  height: 50rpx;
}
.goods-item:nth-child(2) {
  margin-right: 0;
}
.goods-img-image {
  width: 345rpx;
  height: 345rpx;
}
.goods-cant {
  padding: 10rpx 0 20rpx 10rpx;
}
.goods-name {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  font-size: 28rpx;
  color: #333333;
}
.goods-info {
  margin-top: 10rpx;
  color: #999;
}
.goods-price {
  font-size: 22rpx;
}
.goods-now {
  color: #999;
  text-align: center;
}
.goods-now-text {
  line-height: 48rpx;
  font-size: 30rpx;
  font-weight: bold;
}
.goods-former {
  margin-left: 10rpx;
  text-decoration: line-through;
}
.goods-former-text {
  line-height: 48rpx;
}
.goods-like {
  line-height: 48rpx;
}

